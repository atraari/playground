config APP_INIT
    bool "playgroundOS Init"
    default y
    depends on LIB_SEL4 && LIB_MUSL_C && LIB_PLAYGROUND_SYS && LIB_PLAYGROUND && LIB_SEL4_PLAT_SUPPORT && LIB_CPIO && LIB_SEL4_ALLOCMAN && LIB_ELF && LIB_SEL4_VSPACE
    select HAVE_SEL4_APPS
    select APP_BOOTSTRAP
    help
        Simple init startup process.
